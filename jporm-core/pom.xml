		<dependency>
			<groupId>net.sf.ehcache</groupId>
			<artifactId>ehcache-core</artifactId>
			<optional>true</optional>
		</dependency>
		<!-- END - optional scope -->		
			
		<!-- BEGIN - provided scope -->
